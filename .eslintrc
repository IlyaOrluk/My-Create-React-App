{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react",
        "class-property"
    ],
    "rules": {
        "react/state-in-constructor": [0]
    },
    "env": {
        "browser": true,
        "node": true
    }
}